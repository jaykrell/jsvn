#   File:       jmrcpp.make#   Target:     jmrcpp#   Created:    Wednesday, January 28, 2004 03:48:33 AMPPCCPlus = mrcppMAKEFILE        = jmrcpp.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :Includes        = Sym-PPC         = -sym offPPCCOptions     = {Includes} {Sym-PPC} PPCCPlusOptions = {Includes} {Sym-PPC} ### Source Files ###SrcFiles        =  				  j_errno1.c 				  j_macos1.c 				  j_macosx1.c 				  j_malloc1.c 				  j_stdio1.c 				  j46.c### Object Files ###ObjFiles-PPC    =  				  "{ObjDir}j_errno1.c.x" 				  "{ObjDir}j_macos1.c.x" 				  "{ObjDir}j_macosx1.c.x" 				  "{ObjDir}j_malloc1.c.x" 				  "{ObjDir}j_stdio1.c.x" 				  "{ObjDir}j46.c.x"### Libraries ###LibFiles-PPC    =  				  "{SharedLibraries}InterfaceLib" 				  "{SharedLibraries}StdCLib" 				  "{SharedLibraries}MathLib" 				  "{PPCLibraries}StdCRuntime.o" 				  "{PPCLibraries}PPCCRuntime.o" 				  "{PPCLibraries}PPCToolLibs.o" 				  "{PPCLibraries}MrCPlusLib.o" 				  "{PPCLibraries}MrCIOStreams.o"### Default Rules ###.c.x    .c  {쩗ondoBuild}	{PPCCPlus} {depDir}{default}.c -o {targDir}{default}.c.x {PPCCPlusOptions}### Build Rules ###jmrcpp  컴  {ObjFiles-PPC} {LibFiles-PPC} {쩗ondoBuild}	PPCLink 		-o {Targ} 		{ObjFiles-PPC} 		{LibFiles-PPC} 		{Sym-PPC} 		-mf -d 		-t 'MPST' 		-c 'MPS '### Required Dependencies ###"{ObjDir}j_errno1.c.x"    j_errno1.c"{ObjDir}j_macos1.c.x"    j_macos1.c"{ObjDir}j_malloc1.c.x"    j_malloc1.c"{ObjDir}j_stdio1.c.x"    j_stdio1.c"{ObjDir}j46.c.x"    j46.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .x 		{Includes} 		{SrcFiles}