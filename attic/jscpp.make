#   File:       jscpp.make#   Target:     jscpp#   Created:    Wednesday, January 28, 2004 03:33:27 AMCPlus = scppMAKEFILE        = jscpp.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :Includes        = Sym-68K         = -sym offCOptions        = {Includes} {Sym-68K} -model near CPlusOptions    = {Includes} {Sym-68K} -model near ### Source Files ###SrcFiles        =  				  j46cpp.cpp 				  j46.c 				  j_errno1.c 				  j_macos1.c 				  j_malloc1.c 				  j_stdio1.c### Object Files ###ObjFiles-68K    =  				  "{ObjDir}j46.c.o" 				  "{ObjDir}j_errno1.c.o" 				  "{ObjDir}j_macos1.c.o" 				  "{ObjDir}j_malloc1.c.o" 				  "{ObjDir}j_stdio1.c.o"### Libraries ###LibFiles-68K    =  				  "{Libraries}Stubs.o" 				  "{Libraries}MathLib.o" 				  "{CLibraries}StdCLib.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.o" 				  "{Libraries}ToolLibs.o" 				  "{Libraries}Interface.o"### Default Rules ###.c.o    .c  {쩗ondoBuild}	{CPlus} {depDir}{default}.cpp -o {targDir}{default}.cpp.o {CPlusOptions}### Build Rules ###jscpp  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	ILink 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'MPST' 		-c 'MPS ' 		-model near 		-state rewrite 		-compact -pad 0	If "{Sym-68K}" =~ /-sym [nNuU]/		ILinkToSYM {Targ}.NJ -mf -sym 3.2 -c 'sade'	End### Required Dependencies ###"{ObjDir}j46.c.o"    j46.c"{ObjDir}j_errno1.c.o"    j_errno1.c"{ObjDir}j_macos1.c.o"    j_macos1.c"{ObjDir}j_malloc1.c.o"    j_malloc1.c"{ObjDir}j_stdio1.c.o"    j_stdio1.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .o 		{Includes} 		{SrcFiles}