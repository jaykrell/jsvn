#   File:       detect_compiler.make#   Target:     detect_compiler#   Created:    Thursday, January 29, 2004 01:59:42 AMMAKEFILE        = detect_compiler.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :Includes        = Sym-PPC         = -sym offSym-68K         = -sym offPPCCOptions     = {Includes} {Sym-PPC} COptions        = {Includes} {Sym-68K} -model near ### Source Files ###SrcFiles        =  				  detect_compiler.c### Object Files ###ObjFiles-PPC    =  				  "{ObjDir}detect_compiler.c.x"ObjFiles-68K    =  				  "{ObjDir}detect_compiler.c.o"### Libraries ###LibFiles-PPC    =  				  "{SharedLibraries}InterfaceLib" 				  "{SharedLibraries}StdCLib" 				  "{SharedLibraries}MathLib" 				  "{PPCLibraries}StdCRuntime.o" 				  "{PPCLibraries}PPCCRuntime.o" 				  "{PPCLibraries}PPCToolLibs.o"LibFiles-68K    =  				  "{Libraries}Stubs.o" 				  "{Libraries}MathLib.o" 				  "{CLibraries}StdCLib.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.o" 				  "{Libraries}ToolLibs.o" 				  "{Libraries}Interface.o"### Default Rules ###.c.x    .c  {쩗ondoBuild}	{PPCC} {depDir}{default}.c -o {targDir}{default}.c.x {PPCCOptions}.c.o    .c  {쩗ondoBuild}	{C} {depDir}{default}.c -o {targDir}{default}.c.o {COptions}### Build Rules ###detect_compiler 컴 detect_compiler.ppc detect_compiler.68kdetect_compiler.ppc  컴  {ObjFiles-PPC} {LibFiles-PPC} {쩗ondoBuild}	PPCLink 		-o {Targ} 		{ObjFiles-PPC} 		{LibFiles-PPC} 		{Sym-PPC} 		-mf -d 		-t 'MPST' 		-c 'MPS 'detect_compiler.68k  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	ILink 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'MPST' 		-c 'MPS ' 		-model near 		-state rewrite 		-compact -pad 0	If "{Sym-68K}" =~ /-sym [nNuU]/		ILinkToSYM {Targ}.NJ -mf -sym 3.2 -c 'sade'	End### Required Dependencies ###"{ObjDir}detect_compiler.c.x" "{ObjDir}detect_compiler.c.o"    detect_compiler.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .x 		-objext .o 		{Includes} 		{SrcFiles}