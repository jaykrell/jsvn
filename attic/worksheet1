setdirectory z:dev2c j61.ccplus j61.csc j61.cscpp j61.cmrc j61.cmrcpp j61.cppcc j61.cmwcppc j61.cmwc68k j61.copen j61.hopen j61.copen {worksheet}save j61.hsave j61.csave {worksheet}cciigs j61.ccciigs -p j61.cmwcppc j61.cmwc68k j61.cppopen j61.hppopen j61.cppopen {worksheet}save j61.hsave j61.cppsave {worksheet}cciigs -p j61.cppcreatemake make_configure -powerpc -tool make_configure3.ccreatemake configure -68k -tool configure.ccreatemake configure68k -68k -tool configure.ccreatemake configureppc -powerpc -tool configure.copen make_configure3.copen configure.cbuildprogram make_configuremake_configure > configure.cdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configure68k -d C=Cconfigure68kdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configure68k -d C=Cplusconfigure68kdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configure68k -d C=SCconfigure68kdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configure68k -d C=SCppconfigure68kdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configureppc -d PPCC=PPCCconfigureppcdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configureppc -d PPCC='PPCC -dialect cplus'configureppcdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configureppc -d PPCC=MrCconfigureppcdelete -i configure68k configureppc configure.c.o configure.c.xbuildprogram configureppc -d PPCC=MrCppconfigureppcset include System:Library:Frameworks:ApplicationServices.framework:Headersunset includeset sources  'j61.c'set sources  'j61.c j_errno1.c j_malloc1.c j_stdio1.c j_macos1.c'set sources  'j61.c j_errno1.c j_malloc1.c j_stdio1.c'export sourcesc {sources}cplus -z0 {sources}for i in {sources}	mrc {i}endfor i in {sources}	mrcpp {i}endfor i in {sources}	sc {i}endfor i in {sources}	scpp {i}endmwc68k {sources}mwcppc {sources}for i in {sources}	cciigs {i}endppcc {sources}ppcc -dialect cplus {sources}