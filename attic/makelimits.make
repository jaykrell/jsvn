#   File:       makelimits.make#   Target:     makelimits#   Created:    Sunday, March 19, 2006 07:56:00 AMMAKEFILE        = makelimits.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :Includes        = Sym-PPC         = -sym offSym-68K         = -sym offPPCCOptions     = {Includes} {Sym-PPC} COptions        = {Includes} {Sym-68K} -model near ### Source Files ###SrcFiles        =  				  makelimits.c### Object Files ###ObjFiles-PPC    =  				  "{ObjDir}makelimits.c.x"ObjFiles-68K    =  				  "{ObjDir}makelimits.c.o"### Libraries ###LibFiles-PPC    =  				  "{SharedLibraries}InterfaceLib" 				  "{SharedLibraries}StdCLib" 				  "{SharedLibraries}MathLib" 				  "{PPCLibraries}StdCRuntime.o" 				  "{PPCLibraries}PPCCRuntime.o" 				  "{PPCLibraries}PPCToolLibs.o"LibFiles-68K    =  				  "{Libraries}Stubs.o" 				  "{Libraries}MathLib.o" 				  "{CLibraries}StdCLib.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.o" 				  "{Libraries}ToolLibs.o" 				  "{Libraries}Interface.o"### Default Rules ###.c.x    .c  {쩗ondoBuild}	PPCC {depDir}{default}.c -o {targDir}{default}.c.x {PPCCOptions}.c.o    .c  {쩗ondoBuild}	C {depDir}{default}.c -o {targDir}{default}.c.o {COptions}### Build Rules ###makelimits  makelimits.ppc makelimits.68kmakelimits.ppc  컴  {ObjFiles-PPC} {LibFiles-PPC} {쩗ondoBuild}	PPCLink 		-o {Targ} 		{ObjFiles-PPC} 		{LibFiles-PPC} 		{Sym-PPC} 		-mf -d 		-t 'MPST' 		-c 'MPS 'makelimits.68k  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	Link 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'MPST' 		-c 'MPS ' 		-model near ### Required Dependencies ###"{ObjDir}makelimits.c.x" "{ObjDir}makelimits.c.o"    makelimits.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .x 		-objext .o 		{Includes} 		{SrcFiles}