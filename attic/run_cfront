###### CPlus.text - script to invoke CFront###Set Echo	0Set Exit	0Set yFound	0for Param in {"Parameters"}	If "{Param}" =~ /-y/		set yFound	1	else if {yFound}		Set CPlusScratch "{Param}"		Set yFound	0	EndEndIf Â "{CPlusScratch}" ; Set CPlusScratch ':' ; EndCFront -w3 -z3 -y "{CPlusScratch}" {"Parameters"}Set ExitStatus "{Status}"Loop  Break If "{ExitStatus}"  Break If "`Catenate "{CPlusScratch}"C.pipe.cmnd`" !~ /(Å)¨1 ; (Å)¨2 ; (Å)¨3 ; (Å)¨4/  {¨1} ; Set ExitStatus "{Status}" ; Break If "{ExitStatus}"  {¨2} ; Set ExitStatus "{Status}" ; Break If "{ExitStatus}"  {¨3} ; Set ExitStatus "{Status}" ; Break If "{ExitStatus}"  {¨4} ; Set ExitStatus "{Status}" ; Break If "{ExitStatus}"Enddelete -i "{CPlusScratch}"C.pipe.cmnd "{CPlusScratch}"C.pipe.code.c ¶		  "{CPlusScratch}"C.pipe.obj1 "{CPlusScratch}"C.pipe.obj2Exit "{ExitStatus}"