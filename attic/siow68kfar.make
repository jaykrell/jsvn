#   File:       siow68kfar.make#   Target:     siow68kfar#   Created:    Monday, February 9, 2004 04:00:06 AMMAKEFILE        = siow68kfar.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :Includes        = Sym-68K         = -sym offCOptions        = {Includes} {Sym-68K} -model far ### Source Files ###SrcFiles        =  				  configure.c### Object Files ###ObjFiles-68K    =  				  "{ObjDir}configure.c.o"### Libraries ###LibFiles-68K    =  				  "{Libraries}MathLib.far.o" 				  "{CLibraries}StdCLib.far.o" 				  "{Libraries}SIOW.far.o" 				  "{Libraries}MacRuntime.o" 				  "{Libraries}IntEnv.far.o" 				  "{Libraries}ToolLibs.far.o" 				  "{Libraries}Interface.o"### Default Rules ###.c.o    .c  {쩗ondoBuild}	{C} {depDir}{default}.c -o {targDir}{default}.c.o {COptions}### Build Rules ###siow68kfar  컴  {ObjFiles-68K} {LibFiles-68K} {쩗ondoBuild}	ILink 		-o {Targ} 		{ObjFiles-68K} 		{LibFiles-68K} 		{Sym-68K} 		-mf -d 		-t 'APPL' 		-c 'siow' 		-model far 		-state rewrite 		-compact -pad 0	If "{Sym-68K}" =~ /-sym [nNuU]/		ILinkToSYM {Targ}.NJ -mf -sym 3.2 -c 'sade'	Endsiow68kfar  컴  "{RIncludes}"SIOW.r {쩗ondoBuild}	Rez "{RIncludes}"SIOW.r -o {Targ} -append### Required Dependencies ###"{ObjDir}configure.c.o"    configure.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .o 		{Includes} 		{SrcFiles}