Set pathFile "{mpw}"HeadersAndLibs_FolderIf Not `Exists "{pathFile}"`	Echo "{mpw}:Interfaces&Libraries:" > "{pathFile}"EndSet HeadersAndLibs "`Catenate "{pathFile}"`"Export HeadersAndLibsIf Not `Exists -d "{HeadersAndLibs}"`	# Prompt user for correct pathname to Interfaces&Libraries	Set exit 0	Set tmp `GetFileName -d -m "Choose 'Interfaces&Libraries' folder"` ³ dev:null	If "{tmp}"==""		Exit 1		Break	End	If {status} != 0		Alert "HeadersAndLibs:   Problem configuring Interfaces&Libraries location..."		Set HeadersAndLibs "{MPW}"		Exit 0	Else		If Not `Exists -d "{tmp}interfaces"`			Alert "HeadersAndLibs:   Problem configuring Interfaces&Libraries location..."			Set HeadersAndLibs "{MPW}"			Exit 0		End		Echo "{tmp}" > "{pathFile}"		Set HeadersAndLibs	"`Catenate "{pathFile}"`"		Export HeadersAndLibs	EndElse	If Not `Exists -d "{HeadersAndLibs}interfaces"`		Alert "HeadersAndLibs:   Problem configuring Interfaces&Libraries location..."		Set HeadersAndLibs "{MPW}"		Exit 0	EndEnd#	{MWCIncludes} - Directory that contains Metrowerks C Includes.#	This definition might be overridden if UserStartup*Metrowerks exists.	Set MWCIncludes "{HeadersAndLibs}Interfaces:MWCIncludes:"	Export MWCIncludes#	{MWCHeaders} - Variable useful for searching; use without quotes,#	This definition might be overridden if UserStartup*Metrowerks exists.	Set MWCHeaders "'{MWCIncludes}'"	Export MWCHeaders#	{MWPPCLibraries} - Directory that contains Metrowerks C libraries.	Set MWPPCLibraries "{HeadersAndLibs}Libraries:MWPPCLibraries:"	Export MWPPCLibraries#	{MW68KLibraries} - Directory that contains Metrowerks C libraries.	Set MW68KLibraries "{HeadersAndLibs}Libraries:MW68KLibraries:"	Export MW68KLibraries	Set MWLibraries "{HeadersAndLibs}Libraries:"	unset MWLibraries	Export MWLibrariesUnset pathFile