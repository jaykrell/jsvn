alias cat catenatealias ren rename# alias cd jk_cd.mpwalias cd setdirectoryalias pwd directoryalias cp duplicatealias mkdir newfolderalias md newfolderalias ls filesalias del deletealias rm deletealias rmdir deletealias cdup cd ::set MRJSDK "{boot}MRJ SDK 2.2:"export MRJSDKset commands "{commands},{MRJSDK}Tools:MPW Tools:"set commands "{commands},{boot}bin2:"set commands "{commands},{boot}dev2:"export commandsSet AllowMultipleUndo 1export AllowMultipleUndoAddMenu j 'rotatewindows/`' rotatewindowsset CLASSPATH "{MRJSDK}Extensions:JDKToolsClasses.zip,{MRJSDK}Extensions:MRJSDKClasses.zip,{MRJSDK}Extensions:JDKI18nClasses.zip,{SystemFolder}Extensions:MRJ Libraries:MRJClasses:JDKClasses.zip,{SystemFolder}Extensions:MRJ Libraries:MRJClasses:MRJClasses.zip,{SystemFolder}Extensions:MRJ Libraries:MRJClasses:"export CLASSPATHcd mac:dev2:jset SearchWrap 1export SearchWrapAddMenu Build 'Next Error/' 	'Begin; 	 Find /[ 턶]*File/ "{Worksheet}"; 	 Find !0 "{Worksheet}"; 	 Execute "{Worksheet}."; 	 End > Dev:Null  Dev:Null'AddMenu Build 'Previous Error/' 	'Begin; 	 Find \[ 턶]*File\ "{Worksheet}"; 	 Find !0 "{Worksheet}"; 	 Execute "{Worksheet}."; 	 End > Dev:Null  Dev:Null'## performance recommendations for MPW in OS X#set ScreenUpdateDelay 15 # period of cursor animation (ticks)set DontFlushServers 1 # don't FlushVol remote volumes on every tool exitexport ScreenUpdateDelayexport DontFlushServersunset LinkFlags-68kunset LinkFlags-PPCunset LibFiles-68Kunset LibFiles-PPCset LibFiles-68K ""{Libraries}Stubs.o" "{Libraries}MathLib.o" "{CLibraries}StdCLib.o" "{Libraries}MacRuntime.o" "{Libraries}IntEnv.o" "{Libraries}ToolLibs.o" "{Libraries}Interface.o" " && export LibFiles-68Kset LibFiles-68K ""{Libraries}Stubs.o" "{CLibraries}StdCLib.o" "{Libraries}MacRuntime.o" "{Libraries}IntEnv.o" "{Libraries}Interface.o" " && export LibFiles-68Kset LibFiles-PPC ""{SharedLibraries}InterfaceLib" "{SharedLibraries}StdCLib" "{SharedLibraries}MathLib" "{PPCLibraries}StdCRuntime.o" "{PPCLibraries}PPCCRuntime.o" "{PPCLibraries}PPCToolLibs.o" " && export LibFiles-PPCset LinkFlags-68k "-d -t 'MPST' -c 'MPS '"export LinkFlags-68kset LinkFlags-PPC "-d -t 'MPST' -c 'MPS '"export LinkFlags-PPCset LibFiles-MW68K ""{MW68KLibraries}"'MSL MPWRuntime.68K.Lib' "{MW68KLibraries}"'MSL Runtime68K.Lib' "{MW68KLibraries}"'MacOS.Lib' "{MW68KLibraries}"'MSL C.68K MPW(NL_4i_8d).Lib' "{MW68KLibraries}"'MathLib68K (4i_8d).Lib' " && export LibFiles-MW68Kset LibFiles-MWPPC ""{MWPPCLibraries}"'MSL_MPWCRuntime_PPC.Lib' "{MWPPCLibraries}"'MSL_C_PPC_MPW.Lib' "{SharedLibraries}"InterfaceLib "{SharedLibraries}MathLib" " && export LibFiles-MWPPCset LibFiles-MWPPC ""{SharedLibraries}InterfaceLib" "{SharedLibraries}StdCLib" "{SharedLibraries}MathLib" "{PPCLibraries}StdCRuntime.o" "{PPCLibraries}PPCCRuntime.o" "{PPCLibraries}PPCToolLibs.o" " && export LibFiles-MWPPCset LibFiles-MWPPC ""{MWPPCLibraries}"'MSL_MPWCRuntime_PPC.Lib' "{MWPPCLibraries}"'MSL_C_PPC_MPW.Lib' "{SharedLibraries}InterfaceLib" "{SharedLibraries}MathLib" " && export LibFiles-MWPPCsetdirectory mac:dev2:jopen worksheet1